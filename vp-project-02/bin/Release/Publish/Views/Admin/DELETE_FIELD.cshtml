@section styleFile
{
    <link rel="stylesheet" href="@Url.Content("~/Style/admin.css")">
}
@section title
{
    Admin - Delte Field
}
<br><br>
@foreach (var FIELD in ViewBag.GET_FIELD)
{
    using (Html.BeginForm())
    {
        <input type="hidden" value="@FIELD.id" name="id">
        <input type="button" value="&#9642; &nbsp; @FIELD.title" name="title">
        <input type="submit" value="DELETE &nbsp; &#8674;" class="dark-btn DELETE">

    }

}
